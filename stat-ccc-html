;; **
;;; ### Pierres reçues en moyenne par match
;; **

;; @@
(def db-res points-received)
[(plot/bar-chart (first_21 :equipes db-res) (first_21 :points_per_match db-res) :plot-size 950 :aspect-ratio 3)
 (plot/bar-chart (last_21 :equipes db-res) (last_21 :points_per_match db-res) :plot-size 950 :aspect-ratio 3)]
;; @@
