;; **
;;; ### Ends marqués en moyenne par match
;; **

;; @@
(def db-res ends-won)
[(plot/bar-chart (first_21 :equipes db-res) (first_21 :ends_won_per_match db-res) :plot-size 950 :aspect-ratio 3)
 (plot/bar-chart (last_21 :equipes db-res) (last_21 :ends_won_per_match db-res) :plot-size 950 :aspect-ratio 3)]
